<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">

  <%
  if (!config.author) {
    config.author = ""
  }

  var tagTitle = {
    '酷站': '有趣网站大全',
    '游戏': '在线小游戏集合',
    'AI': '趣味AI工具集',
    '工具': '有趣实用工具箱',
  }

  var title = title = config.title;
  if (is_archive()){
    title = theme.title;
  } else if (is_post()){
    title = (page.subTitle || page.title) + ' - ' + theme.title;
  } else if (is_page()){
    title = page.title + ' - ' + theme.title;
  } else if (is_tag()){
    title = (tagTitle[page.tag] || page.tag)  + ' - ' + theme.title;;
  }
  %>
  <title><%= title %></title>
  <% if (config.sitemap) { %>
  <link rel="sitemap" href="<%-config.url + config.sitemap.path %>" />
  <% } %>
  <link rel="canonical" href="<%= url.replace(/index\.html$/, '').replace(/index\.html$/, '') %>">
  <% if (is_page()){ %>
  <meta name="description" content="<%= page.description %>">
  <% } else if (page.content){ %>
  <meta name="description"
    content="<%= strip_html(page.content).replace(/\n/g, " ").replace(/^\s*/, '').replace(/\s*$/, '').substring(0, 150) %>">
  <% } else { %>
  <meta name="description" content="<%= config.description %>">
  <% } %>
  <% if (config.keywords) { %>
  <meta name="keywords" content="<%= config.keywords %>">
  <% } %>
  <meta name="author" content="<%- config.author %>">
  <% if ((is_post() || is_page()) && page.cover) { %>
  <meta property="og:image" content="<%-config.url + page.cover %>">
  <% } else { %>
  <meta property="og:image" content="<%-config.url + theme.thumbnail %>">
  <% } %>
  <meta property="og:site_name" content="<%= config.site_name%>" />
  <meta property="og:type" content="article" />
  <meta property="og:title" content="<%= title %>" />
  <% if (is_page()){ %>
  <meta property="og:description" content="<%= page.description %>">
  <% } else if (page.content){ %>
  <meta property="og:description"
    content="<%= strip_html(page.content).replace(/\n/g, " ").replace(/^\s*/, '').replace(/\s*$/, '').substring(0, 150) %>">
  <% } else { %>
  <meta property="og:description" content="<%= config.description %>">
  <% } %>
  <meta property="og:url" content="<%= url.replace(/index\.html$/, '').replace(/index\.html$/, '') %>" />

  <meta name="twitter:card" content="summary">
  <meta name="twitter:title" content="<%= title %>">
  <% if (is_page()){ %>
  <meta name="twitter:description" content="<%= page.description %>">
  <% } else if (page.content){ %>
  <meta name="twitter:description"
    content="<%= strip_html(page.content).replace(/\n/g, " ").replace(/^\s*/, '').replace(/\s*$/, '').substring(0, 150) %>">
  <% } else { %>
  <meta name="twitter:description" content="<%= config.description %>">
  <% } %>
  <% if ((is_post() || is_page()) && page.cover ) { %>
  <meta name="twitter:image" content="<%-page.cover %>">
  <% } else if((is_post() || is_page()) && page.icon) { %>
    <meta name="twitter:image" content="<%-page.icon %>">
  <% } else { %>
  <meta name="twitter:image" content="<%-config.url + theme.thumbnail %>">
  <% } %>
  <meta name="twitter:url" content="<%= url.replace(/index\.html$/, '').replace(/index\.html$/, '') %>" />

  <script type="application/ld+json">
    {
      "@context" : "https://schema.org",
      "@type" : "WebSite",
      "name" : "<%= config.site_name%>",
      "url" : "https://w3cay.com/"
    }
  </script>

  <!-- Mobile Specific Metas
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- CSS
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <%- css('css/normalize.css') %>
  <%- css('css/skeleton.css') %>
  <%- css('css/custom.css') %>
  <!-- User css -->
  <% if (theme.usercss) { %>
  <%- css(theme.usercss) %>
  <% } %>

  <!-- Favicon
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-icon-180x180.png" />
  <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png" />
  <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png" />
  <link rel="icon" type="image/ico" href="/favicon.ico" />

  <!-- Custom Theme Color Style
  –––––––––––––––––––––––––––––––––––––––––––––––––– -->
  <%- partial('partial/tcolor') %>

 
  <%- partial('partial/third-sdk') %>

</head>

<body>
  <div class="container">
    <div class="row">
      <div>

        <%- partial('partial/header') %>
        <div class="trans">
          <%- body %>
          <%- partial('partial/footer') %>
        </div>
      </div>

    </div>

  </div>
  <script src="<%- theme.nanobar %>"></script>
  <% if (theme.picjs) { %>
  <!-- <script src="<%- theme.picjs %>"></script> -->
  <% } %>
  <script>
    var options = {
      classname: 'nanobar',
      id: 'myNanobar'
    };
    var nanobar = new Nanobar(options);
    nanobar.go(30);
    nanobar.go(76);
    nanobar.go(100);
  </script>

</body>

</html>